{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5","dom"],
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "baseUrl": "/"
  }
}
